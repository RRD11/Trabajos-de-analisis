def insercion(A):
    for i in range(1, len(A)):
        n = A[i]
        j = i-1
        while j >= 0 and n < A[j]:
            A[j+1] = A[j]
            j -= 1
        A[j+1] = n

A = []
print("Ingresa los nÃºmeros uno por uno (escribir 'fin' para terminar):")
while True:
    val = input()
    if val.lower() == 'fin':
        break
    A.append(int(val))
print("Lista no ordenada:", A)
bubble_sort(A)
print("Lista ordenada:", A)
