def burbuja(A):
    n = len(A)
    for i in range(n):
        for j in range(0, n-i-1):
            if A[j] > A[j+1]:
                A[j], A[j+1] = A[j+1], A[j]

A = []
print("Ingresa los n√∫meros uno por uno (escribir 'fin' para terminar):")
while True:
    val = input()
    if val.lower() == 'fin':
        break
    A.append(int(val))
print("Lista ordenada:",A)
