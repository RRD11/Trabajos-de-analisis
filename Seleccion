def seleccion(A):
    for i in range(len(A)):
        n = i
        for j in range(i+1, len(A)):
            if A[j] < A[n]:
                n = j
        A[i], A[n] = A[n], A[i]

# Ejemplo de uso:
A = [64, 34, 25, 12, 22, 11, 90]
print("Lista no ordenada:", A)
seleccion(A)
print("Lista ordenada:", A)
