def seleccion(A):
    for i in range(len(A)):
        n = i
        for j in range(i+1, len(A)):
            if A[j] < A[n]:
                n = j
        A[i], A[n] = A[n], A[i]

A = []
print("Ingresa los nÃºmeros uno por uno (escribir 'fin' para terminar):")
while True:
    val = input()
    if val.lower() == 'fin':
        break
    A.append(int(val))
print("Lista ordenada:", A)
