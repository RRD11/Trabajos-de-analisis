def Rap(A):
    if len(A) <= 1:
        return A
    P = A[len(A) // 2]
    Izq = [x for x in A if x < P]
    M = [x for x in A if x == P]
    Der = [x for x in A if x > P]
    return Rap(Izq) + M + Rap(Der)

#ejemplo de uso
A = []
print("Ingresa los n√∫meros uno por uno (escribir 'fin' para terminar):")
while True:
    val = input()
    if val.lower() == 'fin':
        break
    A.append(int(val))
print("Lista ordenada:", A)
